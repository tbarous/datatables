.application
    font-family 'Nunito'!important
a:hover
    text-decoration none!important
.v-text-field__details
    display none!important
.dg-content-cont
    position fixed
    left 50%
    top 50%
    transform translate(-50%, -50%)
button
    outline none!important
[v-cloak] > *
    display none
[v-cloak]::before
    content "loadingâ€¦"
.v-input__slot
    margin 0!important
.nopacity
    opacity 0!important
.v-label
    margin 0
.active a
    background-color #3490dc!important
.page-item:first-child .page-link
    margin-left 0
    border-top-left-radius 0!important
    border-bottom-left-radius 0!important
.d-picker
    border-radius 0px
    height 48px
    border none
.v-input--selection-controls
    justify-content center
.date-range-picker
    background #fff
    height 48px
    border none
    color #000
    padding 10px
    width 100%
    text-align center
.daterangepicker
    font-family inherit!important
.calendar-table
    th, td
        border 1px solid rgba(0,0,0,0.3)!important
        font-family 'Nunito'
        border-collapse collapse
.disabled .page-link
    background-color black!important
.v-input--selection-controls__input
    margin 0!important
.headline
    font-family 'Nunito'!important
.daterangepicker td.in-range
    background-color lightblue!important
    border-color transparent
    color #000
    border-radius 0
.close-window
    margin 0!important
    position absolute!important
    top 0!important
    right 0!important
div[role=listitem] a
    height 50px!important
.v-list__group__header
    height 50px!important
.fade-enter-active, .fade-leave-active
    transition opacity .3s
.fade-enter, .fade-leave-to
    opacity 0
.theme--dark.v-btn.v-btn--disabled:not(.v-btn--icon):not(.v-btn--flat):not(.v-btn--outline)
    background-color lightgray!important
.notification-title
    font-size 20px!important
    border-bottom 2px solid #fff
    padding-bottom 3px
.notification-content
    padding-top 3px
    font-size 14px!important
    text-align center!important
.notifications
    bottom 10px!important
    right 10px!important
.theme--dark.v-list .v-list__group__header:hover, .theme--dark.v-list .v-list__tile--highlighted, .theme--dark.v-list .v-list__tile--link:hover
    background hsla(0, 0%, 100%, .08)
a:hover
    color #99bffc
a.v-list__tile--active
    color #99bffc!important
.v-list__group__header--active i
    color #99bffc!important
h3
    margin-top 20px
    color #fff
.daterangepicker td.active, .daterangepicker td.active:hover
    background-color #357ebd!important
    border-color transparent
    color #fff
    border-radius 0!important
.pagination a
    border none
.v-toolbar__content
    z-index 999999999999999
.to-top
     position fixed!important
     bottom 10px
     right 10px
a .v-list__tile__action
    justify-content center
.v-list__group__header__prepend-icon
    padding 0!important
    justify-content flex-end!important
.drawer-hr
    border-color hsla(0,0%,100%,.12)
    padding 0
    margin 0
.children a div
    margin-left 20px
aside::-webkit-scrollbar-track
    scrollbar(#F5F5F5)
aside::-webkit-scrollbar
    width 13px!important
    height 7px!important
    background-color #F5F5F5
aside::-webkit-scrollbar-thumb
    scrollbar(#000)
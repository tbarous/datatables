.filter-input
    font-size 13px
    background #fff
    height 50px
    
.table-body
    display contents
.show-how-many
   width 100px
   display inline-block
   margin-left 15px
   margin-right 15px
.pagination-nav
    text-align center
    margin-left auto
    display flex
    align-items center
    justify-content center

